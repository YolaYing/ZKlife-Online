{
  "name": "@hyperoracle/zkgraph-lib",
  "version": "1.0.1",
  "description": "AssemblyScript library for writing zkGraph for Hyper Oracle",
  "keywords": [
    "assemblyscript",
    "zkgraph",
    "hyperoracle"
  ],
  "main": "index.ts",
  "module": "index.ts",
  "types": "index.ts",
  "ascMain": "index.ts",
  "dependencies": {
    "assemblyscript": "^0.27.0"
  },
  "type": "module",
  "scripts": {
    "release": "npm publish --access public",
    "test": "asc test/test.ts -o build/test.wasm --bindings esm && node test/test.js",
    "testblock": "node test/datastructure/test_datastructure.js",
    "format": "prettier --write -c **/*.{js,ts,md}",
    "lint": "prettier -c **/*.{js,ts,md}",
    "weblib-update": "node test/weblib/transform.js"
  },
  "homepage": "https://github.com/hyperoracle/zkgraph-lib",
  "devDependencies": {
    "ethers": "^5.7.2",
    "prettier": "3.0.2",
    "zkwasm-toolchain": "~/Documents/GitHub/zkwasm-toolchain"
  }
}
